{
    "name": "Adu forward backward",
    "version": "2.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "welcome/background.js"
    },
    "action": {
        "default_popup": "ext/index.html"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
    ],
    "permissions": ["storage","activeTab","declarativeContent"]
  }